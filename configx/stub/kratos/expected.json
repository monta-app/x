{
  "courier": {
    "smtp": {
      "connection_uri": "smtps://test:test@mailslurper:1025/?skip_ssl_verify=true",
      "from_address": "no-reply@ory.kratos.sh"
    }
  },
  "config": ["stub/kratos/kratos.yaml"],
  "dsn": "sqlite:///var/lib/sqlite/db.sqlite?_fk=true",
  "hashers": {
    "algorithm": "bcrypt",
    "argon2": {
      "iterations": 2,
      "key_length": 16,
      "memory": "128MB",
      "parallelism": 1,
      "salt_length": 16,
      "expected_duration": "500ms",
      "expected_deviation": "500ms",
      "dedicated_memory": "1GB"
    },
    "bcrypt": {
      "cost":12
    }
  },
  "identity": {
    "default_schema_url": "file:///etc/config/kratos/identity.schema.json"
  },
  "log": {
    "format": "text",
    "leak_sensitive_values": true,
    "level": "debug"
  },
  "secrets": {
    "cipher": ["ipsumipsumipsumipsumipsumipsumip"],
    "cookie": [
      "PLEASE-CHANGE-ME-I-AM-VERY-INSECURE"
    ]
  },
  "ciphers": {
    "algorithm": "noop"
  },
  "selfservice": {
    "default_browser_return_url": "http://127.0.0.1:4455/",
    "flows": {
      "error": {
        "ui_url": "http://127.0.0.1:4455/error"
      },
      "login": {
        "lifespan": "10m",
        "ui_url": "http://127.0.0.1:4455/auth/login"
      },
      "logout": {
        "after": {
          "default_browser_return_url": "http://127.0.0.1:4455/auth/login"
        }
      },
      "recovery": {
        "enabled": true,
        "lifespan": "1h",
        "ui_url": "http://127.0.0.1:4455/recovery"
      },
      "registration": {
        "after": {
          "password": {
            "hooks": [
              {
                "hook": "session"
              }
            ]
          }
        },
        "lifespan": "10m",
        "ui_url": "http://127.0.0.1:4455/auth/registration"
      },
      "settings": {
        "lifespan": "1h",
        "privileged_session_max_age": "15m",
        "ui_url": "http://127.0.0.1:4455/settings",
        "required_aal": "highest_available"
      },
      "verification": {
        "after": {
          "default_browser_return_url": "http://127.0.0.1:4455/"
        },
        "enabled": true,
        "lifespan": "1h",
        "ui_url": "http://127.0.0.1:4455/verify"
      }
    },
    "methods": {
      "link": {
        "enabled": true,
        "config": {
          "lifespan": "1h"
        }
      },
      "oidc": {
        "enabled": true,
        "config": {
          "providers": [
            {
              "id": "google",
              "provider": "google",
              "mapper_url": "file:///etc/config/kratos/oidc.google.jsonnet",
              "client_id": "googleclient@example.com",
              "client_secret": "googlesecret"
            },
            {
              "id": "apple",
              "provider": "apple",
              "mapper_url": "file:///etc/config/kratos/oidc.apple.jsonnet",
              "client_id": "appleclient@example.com",
              "team_id": "apple_team_id",
              "private_key_id": "apple_key_id",
              "private_key": "-----BEGIN PRIVATE KEY-----\nMIIEvAIBADANBgkqhkiG9w0BAQEFAASCBKYwggSiAgEAAoIBAQDKNameCl+1fNV8\nCnx6KIY1tm44ovd+3ImMaY6VFL1tx0rtQowYAtz8e3QiGv6/dadqxk6g7Dbu9HVH\n-----END PRIVATE KEY-----",
              "scope": ["email"]
            }
          ]
        }
      },
      "password": {
        "enabled": true,
        "config": {
          "haveibeenpwned_enabled": true,
          "haveibeenpwned_host": "api.pwnedpasswords.com",
          "max_breaches": 0,
          "ignore_network_errors": true
        }
      },
      "profile": {
        "enabled": true
      },
      "totp": {
        "enabled": false
      },
      "webauthn": {
        "enabled": false
      },
      "lookup_secret": {
        "enabled": false
      }
    },
    "whitelisted_return_urls": [
      "http://127.0.0.1:4455"
    ]
  },
  "serve": {
    "admin": {
      "base_url": "http://kratos:4434/",
      "host": "0.0.0.0",
      "port": 4434,
      "socket": {
        "owner": "",
        "group": "",
        "mode": 493
      }
    },
    "public": {
      "base_url": "http://127.0.0.1:4433/",
      "cors": {
        "allow_credentials": true,
        "allowed_headers": [
          "Authorization",
          "Content-Type",
          "X-Session-Token"
        ],
        "allowed_methods": [
          "POST",
          "GET",
          "PUT",
          "PATCH",
          "DELETE"
        ],
        "allowed_origins": [
          "*"
        ],
        "debug": false,
        "enabled": true,
        "exposed_headers": [
          "Content-Type"
        ],
        "max_age": 0,
        "options_passthrough": false
      },
      "host": "0.0.0.0",
      "port": 4433,
      "socket": {
        "owner": "",
        "group": "",
        "mode": 493
      }
    }
  },
  "cookies": {
    "path": "/",
    "same_site": "Lax"
  },
  "session": {
    "whoami": {
      "required_aal": "highest_available"
    },
    "cookie": {
      "name": "ory_kratos_session",
      "persistent": true
    },
    "lifespan": "24h"
  },
  "expose-metrics-port": 0,
  "sqa-opt-out": false,
  "watch-courier": false,
  "version": "v0.8.0-alpha.3"
}
